package com.mx.Celular;

public class Movie {
	
	private String titulo;
	private int anio;
	private String director;
	private double costo;

	//constructor
	public Movie() {
		
	}

	public Movie(String titulo, int anio, String director, double costo) {
		super();
		this.titulo = titulo;
		this.anio = anio;
		this.director = director;
		this.costo = costo;
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	public int getAnio() {
		return anio;
	}

	public void setAnio(int anio) {
		this.anio = anio;
	}

	public String getDirector() {
		return director;
	}

	public void setDirector(String director) {
		this.director = director;
	}

	public double getCosto() {
		return costo;
	}

	public void setCosto(double costo) {
		this.costo = costo;
	}

	@Override
	public String toString() {
		return "Movie [titulo=" + titulo + ", anio=" + anio + ", director=" + director + ", costo=" + costo + "]\n";
	}
	
	

}
