package Principal;

//Clase Object es la clase raiz de todas las clases
//Si se usa en un HashMap o en una lista (Ejemplo Hashmap<Object, Object> estamos diciendo que
//tanto la clave como el valor puede ser de cualquier tipo de dato o valor)
public interface IMetodos {
	
	void guardar(Object key, Object valor);
	void editar(Object key, Object valur);
	void eliminar(Object key);	
	Object buscar(Object key);
	void mostrar();

}
