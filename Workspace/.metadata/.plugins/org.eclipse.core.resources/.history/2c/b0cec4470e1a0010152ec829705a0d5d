package com.mx.Celular;

public class Peliculas {
	
	private String titulo;
	private int anio;
	private String director;
	private double costo;

	//constructor
	public Peliculas() {
		
	}

	public Peliculas(String titulo, int anio, String director, double costo) {
		super();
		this.titulo = titulo;
		this.anio = anio;
		this.director = director;
		this.costo = costo;
	}
	
	

}
